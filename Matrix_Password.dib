        //  markers[5] = Convert.ToInt32(!letters.Contains<int>(6));
        //  markers[7] = Convert.ToInt32(!letters.Contains<int>(8));

        int[,] Matrix = new int[m,m];
        // for(int i = 0;i < m;i++)
