    public static int Kolvo(char letter, int length)
    {
        int let = (int)letter - (int)letter + 1;
        List<int> l = new List<int>();
        int[,] Matrix = fillMat(l , let);
        printMatrix(Matrix);
        return getKolvo(Matrix , l, length , let);
    }

    public static int getKolvo(int[,] Matrix , List<int> letters,int length, int new_letter)
    {
        int sum = 0;
        int ch = new_letter;
        if(length <= 0) return 0;
        if(length >= 10) return 0;
        letters.Append<int>(new_letter);
        if(letters.Count- length == 0) return 1;
        Console.WriteLine("I was there");
        
        for(int i =0;i<9;i++)
        {
            if(Matrix[i,ch] != 0 )
            {
                sum += getKolvo(fillMat(letters,i) , letters,length , i);
            }
        }
        return sum;
    }
    public static int[,] fillMat(List<int> letters, int new_letter)
    {
        // if( letters.Length != 0)
        Console.WriteLine("!!!");
        int n = new_letter;
        Console.WriteLine(n);
        letters.Add(new_letter);
        int m = 9;
        int[] markers = new int[9];
        for(int i = 0;i< 9;i++)
